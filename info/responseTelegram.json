data Host = Host { ip   :: String
                 , port :: Int
                 }
instance FromJSON Host where
    parseJSON (Object host) = Host <$> host .: "ip"
                                   <*> host .: "port"
    parseJSON _             = mzero

let result = decodeStrict rawJSON
putStrLn $ case result of
    Nothing   -> "Invalid JSON!"
    Just host -> printPretty host

